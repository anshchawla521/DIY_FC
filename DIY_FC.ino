  Serial.println("\t1 : Continue the FC code\n");

  while (true)
  {
    if (Serial.available())
    {
      data = Serial.read();
      switch (data)
      {
      case 48:
        Serial.println("Throttle Pass thru Mode active");
        while (true)
        {
          prev_time = current_time;
