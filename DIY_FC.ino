          // Constrain commands to motors to the max and min limits
          m1_command_PWM = constrain(m1_command_PWM, 125, 250);
          m2_command_PWM = constrain(m2_command_PWM, 125, 250);
          m3_command_PWM = constrain(m3_command_PWM, 125, 250);
